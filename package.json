{
  "name": "tachi-code-themes",
  "version": "1.0.0",
  "description": "Themes registry for Tachi Code",
  "type": "module",
  "scripts": {
    "build": "npx tsx build.ts && prettier --write ./themes/**/*.json",
    "build:strict": "npx tsx build.ts --strict && prettier --write ./themes/**/*.json",
    "import": "npx tsx import-tm-themes.ts",
    "prepare": "husky"
  },
  "author": "David Septimus",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^22.14.1",
    "ajv": "^8.17.1",
    "chalk": "^5.4.1",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "tm-themes": "^1.10.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "eslint": "^9.25.1",
    "husky": "^9.1.7",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0"
  }
}
